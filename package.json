{
  "name": "carpark",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:app && npm run build:server",
    "build:app": "webpack --config=webpack.config.js",
    "build:server": "babel ./server --out-dir ./lib/server --copy-files",
    "build:e2e": "webpack --config=webpack.test.e2e.config.js",
    "start": "cross-env NODE_ENV=production node ./lib/server/server.js",
    "start:dev": "cross-env NODE_ENV=development babel-node ./server/server.js",
    "start:test": "cross-env NODE_ENV=development pm2 start ./server/server.js --interpreter babel-node --name carpark",
    "test": "npm run check:lint && npm run test:unit",
    "test:unit": "karma start",
    "test:e2e": "npm run start:test && npm run build:e2e && protractor tests/e2e/protractor-config.js --suite bvt && npm run stop:test",
    "check:lint": "eslint -f html --ext .js --ext .jsx src/ -o tests/out/lint-report.html",
    "update:webdriver": "webdriver-manager update",
    "stop:test": "pm2 kill"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "cross-env": "3.1.3",
    "express": "4.16.2",
    "prop-types": "15.5.7",
    "react": "15.6.2",
    "react-addons-pure-render-mixin": "15.3.2",
    "react-dom": "15.6.2",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.24.0",
    "babel-plugin-istanbul": "4.1.3",
    "babel-plugin-module-resolver": "2.3.0",
    "babel-plugin-rewire": "1.0.0-rc-3",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-nodejs6": "1.0.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.23.0",
    "enzyme": "2.8.2",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0",
    "jasmine-core": "2.6.1",
    "jasmine-enzyme": "3.1.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "2.0.3",
    "node-sass": "4.3.0",
    "redux-mock-store": "1.4.0",
    "pm2": "2.4.0",
    "protractor": "5.2.2",
    "protractor-jasmine2-html-reporter": "^0.0.7",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.5.1",
    "react-test-renderer": "15.5.4",
    "sass-loader": "6.0.3",
    "style-loader": "0.13.0",
    "svg-react-loader": "0.3.2",
    "webpack": "2.4.1",
    "webpack-dev-middleware": "1.10.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/desmondgong/carpark"
  }
}
